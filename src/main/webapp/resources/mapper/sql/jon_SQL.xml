<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="jon">

	<select id="select_jon_List" resultType="hashmap" parameterType="Integer">
			<![CDATA[
				SELECT
				    *
				FROM
				    hangul.jon	
				WHERE 
					jon_num=#{jon_num} order by jon_stroke_num;
							
			]]>
	</select>
		

</mapper>