<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cho">

	<select id="select_cho_List" resultType="hashmap" parameterType="Integer">
			<![CDATA[
				SELECT
				    *
				FROM
				    hangul.cho	
				WHERE 
					cho_num=#{cho_num} order by cho_stroke_num;
							
			]]>
	</select>
		

</mapper>